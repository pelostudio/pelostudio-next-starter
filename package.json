{
  "name": "pelostudio-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "next dev",
    "build": "next build",
    "serve": "next start",
    "build:static": "next build && next export",
    "serve:static": "npx serve ./out",
    "storybook": "start-storybook -p 6006 -c ./src/storybook/ -s ./src/storybook/public",
    "build-storybook": "build-storybook -c ./src/storybook/ && rsync -avz --progress ./src/storybook/public/ ./storybook-static/",
    "generate-component": "node .generate-component $1",
    "test": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lorem-ipsum": "^2.0.3",
    "next": "10.1.3",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.2",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.169",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.2.0",
    "storybook-addon-next-router": "^2.0.4",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ],
    "./**/*.{css,scss}": [
      "stylelint --fix"
    ]
  },
  "browserslist": [
    "defaults, >=.1% in FR"
  ]
}
