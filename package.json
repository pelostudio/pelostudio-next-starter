{
  "name": "pelostudio-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "next dev",
    "build": "next build",
    "serve": "next start",
    "build:static": "next build && next export",
    "serve:static": "npx serve ./out",
    "storybook": "start-storybook -p 6006 -s ./.storybook/public",
    "build-storybook": "build-storybook && rsync -avz --progress .storybook/public/ ./storybook-static/",
    "generate-component": "node .generate-component $1"
  },
  "dependencies": {
    "husky": "^4.3.8",
    "next": "10.0.9",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.0",
    "storybook-addon-next-router": "^2.0.4",
    "style-loader": "^1.3.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ],
    "./**/*.{css,scss}": [
      "stylelint --fix"
    ]
  }
}
